Bundle-Name: Tasks Web
Bundle-SymbolicName: com.liferay.tasks.web
Bundle-Version: 1.0.0

Service-Component: *

Import-Package: \
	javax.portlet;version='[2.0,3)',\
	javax.servlet;version='[2.5,4)',\
	*
	
	
Include-Resource: \
	src/main/resources,\
	resource-actions=src/main/resources/META-INF/resource-actions,\
	META-INF/resources=src/main/resources/META-INF/resources

-dsannotations: *
Private-Package: \
	com.liferay.tasks.web.portlet,\
	com.liferay.tasks.web.portlet.actions